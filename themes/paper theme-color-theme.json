{
	"name": "paper theme",
	"type": "light",
	"colors": {
		"editor.background": "#fffbf6",
		"editor.foreground": "#312F2F",
		"editorLineNumber.foreground": "#918D8D",
		"editorLineNumber.activeForeground": "#312F2F",
		"sideBar.background": "#fffbf6",
		"tab.inactiveBackground": "#fffbf6",
		"tab.inactiveForeground": "#918D8D",
		"editorGroupHeader.tabsBackground": "#fffbf6",
		"titleBar.activeBackground": "#312F2F",
		"titleBar.inactiveBackground": "#918D8D",
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#918D8D"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#918D8D"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#918D8D"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#B15065"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#312F2F"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#312F2F"
			}
		},
		{
			"scope": [
				"storage.type.haskell"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"variable.parameter.function.language.python"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"support.type.exception.python"
			],
			"settings": {
				"foreground": "#af1d3c",
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#312F2F"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#312F2F"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#312F2F"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#312F2F"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#B15065"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#312F2F"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#918D8D"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#312F2F"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#918D8D"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#312F2F"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#312F2F"
			}
		},
	]
}